#!/usr/bin/env bash
source /etc/profile
source ~/.profile

[ -f ~/.xsession.local ] && source ~/.xsession.local

export TERMINAL=roxterm

# start programms in background
numlockx &
pulseaudio -D &
#trayion &
synergys &
xkbevd -bg &
setxkbmap us
xmodmap ~/.Xmodmap &
roxterm &
xset r rate 280 30 &
xset b 100 &

xkbevd -bg &

#xset m 3 1
#xset -b
#skype &

# linclude local

# start window manager
exec i3
